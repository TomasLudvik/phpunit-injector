{
    "name": "zalas/phpunit-injector",
    "description": "Injects services from a PSR-11 dependency injection container to PHPUnit test cases",
    "type": "library",
    "require": {
        "php": "^7.4,<8.0",
        "phpunit/phpunit": "^9.0",
        "psr/container": "^1.0",
        "zalas/injector": "^2.0"
    },
    "require-dev": {
        "symfony/config": "^3.4 || ^4.4 || ^5.0",
        "symfony/dependency-injection": "^3.4 || ^4.4 || ^5.0",
        "symfony/http-kernel": "^3.4 || ^4.4 || ^5.0",
        "zalas/phpunit-globals": "^2.0",
        "symfony/framework-bundle": "^3.4 || ^4.4 || ^5.0",
        "zalas/phpunit-doubles": "^1.5",
        "phpspec/prophecy": "^1.9",
        "phpspec/prophecy-phpunit": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Zalas\\Injector\\PHPUnit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zalas\\Injector\\PHPUnit\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Zalas",
            "email": "jakub@zalas.pl"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
